<?xml version="1.0" encoding="utf-8"?>
<configuration status="OFF">
    <appenders>
        <!--输出到控制台-->
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{YYYY-MM-dd HH:mm:ss} %-5level [%thread] [%logger{36}.%method-%L] - %msg%n"/>
        </Console>
    </appenders>

    <loggers>
        <root level="info">
            <appender-ref ref="console"/>
        </root>
    </loggers>
</configuration>